<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.media.MediaView?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.scenebuilder.MainMenuController">
   <children>
      <Text layoutX="330.0" layoutY="71.0" scaleX="5.0" scaleY="5.0" scaleZ="5.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome To VARWIKI" />
      <Button fx:id="createButton" layoutX="14.0" layoutY="112.0" mnemonicParsing="false" onAction="#handleCreate" text="Create New Creation" />
      <ScrollPane layoutX="14.0" layoutY="152.0" prefHeight="381.0" prefWidth="243.0">
         <content>
            <ListView fx:id="videoListView" prefHeight="382.0" prefWidth="244.0" />
         </content>
      </ScrollPane>
      <HBox layoutX="400.0" layoutY="534.0" prefHeight="49.0" prefWidth="243.0">
         <children>
            <Button fx:id="backwardButton" mnemonicParsing="false" onAction="#handleBackward" text="&lt;&lt;" />
            <Button fx:id="_multiButton" defaultButton="true" mnemonicParsing="false" onAction="#handleVideoMultiButton" text="Play" />
            <Button fx:id="forwardButton" mnemonicParsing="false" onAction="#handleForward" text="&gt;&gt;" />
            <Button fx:id="muteButton" mnemonicParsing="false" onAction="#handleMute" text="Mute" />
         </children>
      </HBox>
      <AnchorPane fx:id="player" layoutX="319.0" layoutY="173.0" prefHeight="340.0" prefWidth="387.0">
         <children>
            <MediaView fx:id="_player" fitHeight="200.0" fitWidth="200.0" layoutX="94.0" layoutY="63.0" />
            <Label fx:id="_currentLabel" layoutX="77.0" layoutY="36.0" text="Currently Playing:" />
            <Slider fx:id="_slider" layoutX="94.0" layoutY="247.0" prefHeight="16.0" prefWidth="200.0" />
         </children></AnchorPane>
   </children>
</AnchorPane>
