<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.scenebuilder.MainMenuController">
   <children>
      <AnchorPane prefHeight="600.0" prefWidth="800.0">
         <children>
            <Button fx:id="deleteButton" layoutX="193.0" layoutY="493.0" mnemonicParsing="false" onAction="#handleDeleteVideo" text="Delete" />
            <Text layoutX="21.0" layoutY="64.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome To VARWIKI">
               <font>
                  <Font size="62.0" />
               </font>
            </Text>
            <Button fx:id="createButton" layoutX="31.0" layoutY="493.0" mnemonicParsing="false" onAction="#handleCreate" text="Create New Creation" />
            <HBox fx:id="_hbox" layoutX="31.0" layoutY="108.0" spacing="30.0">
               <children>
                  <ScrollPane prefHeight="385.0" prefWidth="325.0">
                     <content>
                        <ListView fx:id="videoListView" onMouseClicked="#handleSelectionChange" prefHeight="382.0" prefWidth="301.0" />
                     </content>
                  </ScrollPane>
               </children>
            </HBox>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
